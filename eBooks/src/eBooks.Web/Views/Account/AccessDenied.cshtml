
<div class="mt-2 bg-light">
    <div class="authentication-inner row m-0">
        <div class="d-none d-lg-flex  align-items-center">
            <div class="flex-row text-center mx-auto">
                <img src="~/images/accessDenied .jpg" alt="Auth Cover Bg color" width="520" class="img-fluid authentication-cover-img" data-app-light-img="pages/login-light.png" data-app-dark-img="pages/login-dark.png">
                <div class="mx-auto">
                    <div class="w-px-400 mx-auto">
                        <div class="app-brand mb-4">
                            <a href="index.html" class="app-brand-link gap-2 mb-2">
                                <span class="app-brand-logo demo">
                                </span>
                            </a>
                        </div>
                        <h1 class="text-danger mt-5">خطا 403</h1>
                        <h2 class="mt-3">Unauthorized access.</h2>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <input hidden name="returnUrl" value="@ViewData["ReturnUrl"]"/>
                        <div class="form-group mt-3">
                            <h5> You do not have permission to access this page.</h5>
                        </div>
                        <a href="/" class="btn btn-primary btn-lg d-grid mt-3 mb-3">Return to the main page</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="~/Simat/css/icomoon/styles.css">
    <link rel="stylesheet" href="~/Simat/css/bootstrap.css">
    <link rel="stylesheet" href="~/Simat/css/fontawesome/styles.min.css">
    <link rel="stylesheet" href="~/Simat/css/core.css">
    <link rel="stylesheet" href="~/Simat/css/components.css">
    <link rel="stylesheet" href="~/Simat/css/colors.css">
    <link rel="stylesheet" href="~/Simat/css/custom-style.css">


    <link rel="stylesheet" href="~/Simat/Scripts/modernizr-*">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

<script src="~/Simat/Scripts/jquery-3.1.1.js"></script>
<script src="~/Simat/Scripts/jquery.unobtrusive-ajax.js"></script>
<script src="~/Simat/assets/js/jquery.min.js"></script>
<script src="~/Simat/assets/js/bootstrap.min.js"></script>
<script src="~/Simat/assets/plugins/blockui.min.js"></script>
<script src="~/Simat/Scripts/plugins/jquery.mvc.deleteItem.js"></script>
<script src="~/Simat/Scripts/plugins/jquery.mvc.getJson.js"></script>
<script src="~/Simat/Scripts/plugins/jqeury.stylesheet.js"></script>
<script src="~/Simat/Scripts/plugins/CheckConnection.js"></script>
<script src="~/Simat/Scripts/plugins/jquery.number.js"></script>
<script src="~/Simat/Scripts/plugins/jquery.cookie.js"></script>
<script src="~/Simat/assets/pages/layout_fixed_native.js"></script>
<script src="~/Simat/Scripts/plugins/tableHeadFixer.js"></script>

<script src="~/Simat/assets/js/jquery_ui/core.min.js"></script>
<script src="~/Simat/assets/js/jquery_ui/effects.min.js"></script>


<script src="~/Simat/trees/fancytree_all.min.js"></script>
<script src="~/Simat/trees/fancytree_childcounter.js"></script>

<script src="~/Simat/assets/plugins/uploaders/fileinput.js" type="text/javascript"></script>
<script src="~/Simat/assets/plugins/uploaders/fa.js" type="text/javascript"></script>
<script src="~/Simat/assets/js/bootstrap.min.js" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
</body>
</html>